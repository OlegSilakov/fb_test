require 'minitest/autorun'
require_relative 'algoritm'

class  AlgoritmTest < MiniTest::Unit::TestCase

  def test_algoritm_1
    assert_equal [2,6], Algoritm.miss(2,6)
  end

  def test_algoritm_2
    assert_equal [2,20], Algoritm.miss(2,20)
  end

  def test_algoritm_3
    assert_equal [4,5], Algoritm.miss(4,5)
  end

  def test_algoritm_4
    assert_equal [8,10], Algoritm.miss(8,10)
  end

  def test_algoritm_5
    assert_equal [30,40], Algoritm.miss(30,40)
  end

  def test_algoritm_6
    assert_equal [1,3], Algoritm.miss(1,3)
  end

  def test_algoritm_7
    assert_equal [47,50], Algoritm.miss(47,50)
  end

end